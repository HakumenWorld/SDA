#include <iostream>
#include <queue>
using namespace std;

int main() {
    // Mendeklarasikan antrean untuk pasien
    queue<string> antreanPasien;

    // Menambahkan pasien ke antrean
    antreanPasien.push("Ayunda Risu");
    antreanPasien.push("Febi");
    antreanPasien.push("Kobo Kanaeru");

    // Mengeluarkan pasien dari antrean
    cout << "Pasien yang dilayani: " << antreanPasien.front() << endl;
    antreanPasien.pop();

    return 0;
}
